                        <MenuItem styleClass="menu-item" mnemonicParsing="false" text="Junior Suite" onAction="#abrirMostrarEstadisticas"/>
                    </items>
                </MenuButton>
                <MenuButton layoutX="616.0" layoutY="7.0" mnemonicParsing="false" styleClass="menu-button" text="WebView" textFill="WHITE">
                    <items>
                        <MenuItem styleClass="menu-item" mnemonicParsing="false" text="Traductor" onAction="#abrirTraductor"/>
                    </items>
                </MenuButton>
            </children>
        </AnchorPane>
    </top>
